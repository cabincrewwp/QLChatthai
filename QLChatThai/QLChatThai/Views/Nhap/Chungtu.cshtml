@{
    ViewBag.Title = "Chứng từ";
 }

<h2>Lập Chứng Từ</h2>
<br/>
@using (Html.BeginForm("Lapchungtu", "Nhap",FormMethod.Post))
{
    <div>
        <div class="row">
            <div class="form-group col-sm-3">
                <label>Từ ngày</label>
            </div>
            <div class="form-group col-sm-3">
                <input name="txttungay" class="nn-date" />
            </div>
            <div class="form-group col-sm-3">
                <label>Tới ngày</label>
            </div>
            <div class="form-group col-sm-3">
                <input name="txttoingay" class="nn-date"/>
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                <div class="col-sm-12">
                    <input type="submit" value="Submit" class="btn btn-default"/>
                </div>
            </div>
        </div>
    </div>
    <div>
    @Html.ActionLink("Back to List", "Index")
</div>
}
